<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_colorRipple" format="color" />
        <attr name="fab_shadow" format="boolean" />
    </declare-styleable>
    <!-- tabbar -->
    <attr name="tabbar_defStyle" format="reference"/>
    <declare-styleable name="TabBar">
        <attr name="tab_item_padding" format="dimension"/>
        <attr name="tab_item_padding_top" format="dimension"/>
        <attr name="tab_item_padding_bottom" format="dimension"/>
        <attr name="tab_item_padding_left" format="dimension"/>
        <attr name="tab_item_padding_right" format="dimension"/>
        <attr name="tab_item_padding_horizontal" format="dimension"/>
        <attr name="tab_item_padding_vertical" format="dimension"/>
        <attr name="tab_title_offset" format="dimension"/>
        <attr name="tab_has_indicator" format="boolean"/>
        <attr name="tab_title_textSize" format="integer"/>
        <attr name="tab_mode" format="enum">
            <enum name="text" value="0"/>
            <enum name="icon" value="1"/>
            <enum name="text_icon" value="2"/>
        </attr>
    </declare-styleable>
    <!-- tabbar -->
</resources>